      <thead>
        <tr>
          <th>Nombre</th>
          <th>Apertura</th>
          <th>Finalizaci√≥n</th>
          <th>Estado</th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        {{#each projectList}}
        <tr>
          <td><a href="/projects/{{id}}" title="Detalle del Proyecto">{{name}}</a></td>	  
